<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="leitorrss.cnbatalha.leitorrss" >

    <uses-permission android:name="android.permission.INTERNET"></uses-permission>

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme"
        android:name="com.activeandroid.app.Application">
        <activity
            android:name=".activity.MainActivity"
            android:label="@string/title_activity_main" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.NoticiasActivity"
                    android:label="@string/title_activity_noticias">

        </activity>

        <meta-data android:name="DATABASE" android:value="rss.db" />
        <meta-data android:name="VERSION" android:value="2" />
        <meta-data android:name="QUERY_LOG" android:value="true" />
        <meta-data android:name="DOMAIN_PACKAGE_NAME" android:value="leitorrss.cnbatalha.leitorrss.model" />

        <meta-data android:name="AA_DB_NAME" android:value="leitor-noticias-db.db" />
        <meta-data android:name="AA_DB_VERSION" android:value="4" />
        <meta-data
            android:name="AA_MODELS"
            android:value="leitorrss.cnbatalha.leitorrss.model.Categoria" />
    </application>

</manifest>
